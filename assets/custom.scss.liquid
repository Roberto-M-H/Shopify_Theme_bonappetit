.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
  font-family: 'Futura PT Demi';
}
.h2, h2 {
  font-size: 45px;
  line-height: 52px;
  letter-spacing: 0.8px;
  color: #000000;
}
.template-index{
  .site-header__inner{
  	background:#000;
  }
}
.page-width, .page-width--wide {
  max-width: 1140px;
}
.site-header__toggle-nav .icon {
  padding: 0 10px;
}
.site-header__cart .icon, .site-header__search .icon {
  padding-top: 0;
  padding-bottom: 0;
}
input:focus, select:focus, textarea:focus {
  outline: none;
}
.icon-hamburger:before {
  content: '';
  background: url(https://cdn.shopify.com/s/files/1/0580/8085/4213/files/reorder_24px.svg?v=1624569618) 0 0 no-repeat;
  width: 16px;
  height: 12px;
  display: block;
}
.icon-search:before{
  content: '';
  background: url(https://cdn.shopify.com/s/files/1/0580/8085/4213/files/search.svg?v=1624569822) 0 0 no-repeat;
  width: 16px;
  height: 16px;
  display: block;
}
.icon-cart:before{
  content: '';
  background: url(https://cdn.shopify.com/s/files/1/0580/8085/4213/files/shopping_cart_24px.svg?v=1624569729) 0 0 no-repeat;
  width: 18px;
  height: 18px;
  display: block;
}
.site-header__link {
  vertical-align: middle;
  margin-top: -3px;
}
.site-header__cart .icon, .site-header__search .icon {
  display: block;
}
.btn {
  border: 2px solid #000;
  background: transparent;
  color: #000;
  border-radius:0;
  font-size: 12px;
  font-family: 'Futura PT';
  font-weight: 500;
  letter-spacing: 0.14em;
  padding: 14px 37px;
  margin-top:36px;
  &:hover{
    background:#000;
    color:#fff;
    opacity:1 !important;
  }
}
.food-culture {
  padding: 0 60px;
  .about_tag {
    border: 1px solid #000;
    display: block;
    text-align: center;
    max-width: 85px;
    margin: 0 auto;
    position: relative;
    z-index: 2;
    p {
      margin: 0 auto;
      font-size: 12px;
      line-height: 26px;
      text-align: center;
      letter-spacing: 0.14em;
      text-transform: uppercase;
      font-family: 'Futura PT';
      font-weight: 500;
      color: #000000;
    }
  }
  .feature-row {
    margin: -14px auto 0;
    .feature-row__content {
      text-align: center;
      max-width: 430px;
      margin: 0 auto !important;
      h2 {
        font-size: 45px;
        line-height: 52px;
        text-align: center;
        letter-spacing: 0.8px;
        margin: 0 auto 20px;
      }
      p {
        font-weight: normal;
        font-size: 17px;
        line-height: 26px;
        text-align: center;
        color: #000000;
        margin: 0 auto 0;
        font-family: 'Futura PT Demi';
      }
    }
  }
}


.site-footer {
  border-top: 40px solid #333333;
  padding: 90px 20px 86px;
  text-align: center;
  .site-footer__logo-link {
    display: inline-block;
    img {
      width: 123px;
    }
  }
  .footer_logo{
    margin: 0 auto 30px;
  }
  .footer_btn {
    margin-bottom: 33px;
    a.btn {
      padding: 14px 15px;
    }
  }
  .site-footer__linklist {
    display: inline-block;
    margin: 0 auto 42px;
    li {
      display: block;
      a {
        font-weight: normal;
        font-size: 10px;
        line-height: 14px;
        padding: 10px 0;
        text-align: center;
        letter-spacing: 2px;
        text-transform: uppercase;
      }
    }
  }
  .site-footer__copyright {
    font-size: 11px;
    line-height: 19px;
    letter-spacing: 0.6px;
  }
}
.template-product{
  .product__photo-single {
    img {
      width: 100%;
      height: calc(100vh - 51px);
      object-fit: cover;
      object-position: center;
      min-height: 675px;
    }
  }
  ul.slick-dots {
    bottom: 100px;
    li button:before {
      font-size: 8px;
      color: rgba(0, 0, 0, 0.3);
      opacity: 1;
    }
    li.slick-active button:before{
      opacity: 1;
      color:#000;
    }
  }
  .spr-badge {
    margin-bottom: 10px;
    .spr-badge-caption {
      font-family: 'Futura PT';
      font-weight: 500;
      font-size: 15px;
      line-height: 20px;
      color: #000;
      text-transform: capitalize;
      vertical-align: middle;
    }
    .spr-badge-starrating {
      margin: 0 6px 0 0;
      vertical-align: middle;
    }
    .spr-icon.spr-icon-star-empty {
      opacity: 1;
    }
    [class^="spr-icon-"]:before, [class*=" spr-icon-"]:before {
      font-size: 24px;
      margin: 0 2px;
    }
  }
}

.product__details{
  .product__details-content {
    max-width: 400px;
    padding: 40px 0;
    overflow: hidden;
    .grid__item {
      padding-left: 60px;
    }
    .h2 {
      font-size: 45px;
      line-height: 52px;
      letter-spacing: 0.8px;
      margin: 0 0 23px;
    }
    p.product__price {
      font-size: 17px;
      line-height: 26px;
      font-family: 'Futura PT Demi';
      color: #000;
    }
    .shopify-payment-button {
      display: none !important;
    }
    .product-single__description {
      font-family: 'Radley', serif;
      font-size: 16px;
      line-height: 26px;
      color: #000000;
    }
    .btn--add-to-cart {
      text-transform: uppercase;
      display: inline-block;
      margin:0;
      width: auto;
    }
    .minus-btn, .plus-btn {
      cursor: pointer;
      font-size: 12px;
      color: #000;
    }
    .product__quantity-label {
      display: block;
      font-weight: 500;
      font-size: 15px;
      line-height: 20px;
      color: #000000;
      font-family: 'Futura PT';
      padding: 2px 0 5px;
    }
    .product__quantity {
      float: none;
      margin: 0 0 20px;
    }
    .quantity-input {
      text-align: center;
      padding: 0;
      width: auto;
      font-family: 'Futura PT Demi';
      font-size: 17px;
      line-height: 26px;
      width: 20px;
    }
  }
}
.product-recommendations {
  padding: 0 54px 60px;
  h2 {
    margin: 0 auto 20px;
  }
  .collection-grid {
    display: flex;
    justify-content: center;
    .product-item.medium-up--one-quarter {
      width: 16.6666667%;
      padding: 0 6px;
      .product-item__link{
        &:hover {
          .product-item__image{
          	opacity:1;
          }
        }
      }
      .product-item__image--margins {
        margin: 0;
        .product-item__meta {
          position: static;
          opacity: 1;
          &:before {
            display: none;
          }
          .product-item__meta__inner {
            position: static;
            transform: inherit;
            padding: 0;
            .product-item__price-wrapper {
              display: none;
            }
            p.product-item__title {
              font-size: 17px;
              line-height: 26px;
              text-align: center;
              color: #000000;
              font-family: 'Futura PT Demi';
              margin: 12px auto;
            }
          }
        }        
      }
    }
  }
}
.product__reviews {
  margin: 40px 60px 0;
  h2 {
    margin: 0 auto 20px;
  }
  .spr-header-title {
    display: none;
  }
  #shopify-product-reviews {
    border-top: 1px solid #000000;
    margin: 42px 0;
    .spr-container {
      padding: 26px 0;
      .spr-form-title {
        font-family: 'Futura PT Demi';
        font-size: 28px;
      }
      .spr-form-label {
        font-family: 'Futura PT Demi';
        color: #000;
        font-size: 14px;
      }
      .spr-form-input-text, .spr-form-input-email, .spr-form-input-textarea {
        font-family: 'Futura PT';
      }
      input.spr-button {
        text-transform: uppercase;
        font-family: 'Futura PT Demi';
      }
      .spr-review-content-body {
        font-size: 17px;
        line-height: 26px;
        color: #000000;
        font-family: 'Futura PT Demi';
      }
      .spr-review-header {
        display: flex;
        flex-direction: column;
        .spr-review-header-byline {
          order: 1;
          font-size: 17px;
          font-style: normal;
          line-height: 26px;
          color: #000;
          font-family: 'Futura PT Demi';
          margin: 0 0 10px;
        }
        .spr-review-header-starratings {
          order: 2;
          margin: 0;
          display: block;
          .spr-icon {
            font-size: 18px;
          }
        }
        .spr-review-header-title {
          order: 3;
          font-size: 30px;
          line-height: 32px;
          letter-spacing: 0.8px;
          color: #000000;
          font-family: 'Futura PT Demi';
          margin: 30px 0 15px;
        }
      }

    }
    .spr-summary-actions-newreview {
      border: 1px solid #DADADA;
      font-size: 17px;
      line-height: 26px;
      color: #000000;
      font-family: 'Futura PT Demi';
      padding: 11px 30px;
      vertical-align: middle;
      &:before {
        content: '';
        background: #000;
        -webkit-mask-image: url(https://cdn.shopify.com/s/files/1/0580/8085/4213/files/pencil.svg?v=1624623302);
        mask-image: url(https://cdn.shopify.com/s/files/1/0580/8085/4213/files/pencil.svg?v=1624623302);
        width: 18px;
        height: 22px;
        -webkit-mask-repeat: no-repeat;
        display: inline-block;
        vertical-align: middle;
        margin-right: 10px;
      }
      &:hover{
      	background:#000;
        border-color:#000;
        color:#fff;
        opacity:1;
        &:before {
         background:#fff; 
        }
      }
    }
  }
}

